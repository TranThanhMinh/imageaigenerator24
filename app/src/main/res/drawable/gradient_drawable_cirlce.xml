<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:shape="rectangle">
        <shape>
            <gradient
                android:startColor="#fff00dbe"
                android:endColor="#ff3771c8"
                android:angle="90"
                android:type="linear"/>
            <corners android:radius="100dp"/>
        </shape>
    </item>
    <item
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape>
            <solid android:color="#ffffff"/>
            <corners android:radius="100dp"/>
        </shape>
    </item>
</layer-list>
